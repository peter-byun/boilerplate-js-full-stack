{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"pnpm dev:gateway\" \"pnpm dev:worker\"",
    "dev:gateway": "nest start --watch --entryFile main.gateway",
    "dev:worker": "nest start --watch --entryFile main.worker",
    "build": "nest build",
    "start": "concurrently \"pnpm start:gateway\" \"pnpm start:worker\"",
    "start:gateway": "node dist/main.gateway",
    "start:worker": "node dist/main.worker",
    "start:debug": "nest start --debug --watch",
    "start:prod": "concurrently \"NODE_ENV=production node dist/main.gateway\" \"NODE_ENV=production node dist/main.worker\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "migration:create": "mikro-orm migration:create",
    "migration:up": "mikro-orm migration:up",
    "migration:down": "mikro-orm migration:down",
    "migration:list": "mikro-orm migration:list",
    "migration:pending": "mikro-orm migration:pending"
  },
  "dependencies": {
    "@mikro-orm/core": "^6.5.9",
    "@mikro-orm/migrations": "^6.5.9",
    "@mikro-orm/nestjs": "^6.1.1",
    "@mikro-orm/postgresql": "^6.5.9",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/microservices": "^11.1.7",
    "@nestjs/platform-express": "^11.0.0",
    "@repo/api": "workspace:*",
    "ioredis": "^5.8.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@mikro-orm/cli": "^6.5.9",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/jest-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/express": "^4.17.17",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.0",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.5.4"
  }
}
